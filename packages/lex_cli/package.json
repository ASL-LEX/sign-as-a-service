{
  "name": "lex_cli",
  "version": "0.0.0",
  "description": "CLI for interacting with LEX interface",
  "author": "cbolles @cbolles",
  "bin": {
    "lex_cli": "./bin/run"
  },
  "homepage": "https://github.com/hicsail/lex_cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "hicsail/lex_cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript-graphql-request": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@oclif/test": "^2.3.26",
    "@types/chai": "^4",
    "@types/node": "^16.18.38",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "lex_cli",
    "dirname": "lex_cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md",
    "introspection": "graphql-codegen --config graphql-codegen.yml"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/hicsail/lex_cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
